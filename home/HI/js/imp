function initMap() {
    var uluru = {lat: 28.6751977, lng: 77.1102413}; //enter latitude and longitude of location
    var map = new google.maps.Map(document.getElementById('map'), {
        zoom: 14,
        center: uluru,
        scrollwheel: false
    });
    var marker = new google.maps.Marker({
        position: uluru,
        map: map
    });
}
